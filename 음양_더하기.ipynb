        "def solution(absolutes, signs):\n",
        "    answer = 0\n",
        "    for i, j in zip(absolutes, signs):\n",
        "        if j: #true: 참\n",
        "            answer += i\n",
        "        else:\n",
        "            answer -= i\n",
        "    return answer"
      ],
